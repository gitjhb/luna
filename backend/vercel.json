{
  "version": 2,
  "installCommand": "pip install -r requirements-vercel.txt",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "MOCK_REDIS": "true",
    "VECTOR_DB_PROVIDER": "none"
  }
}
