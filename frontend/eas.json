{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_REVENUECAT_API_KEY": "@EXPO_PUBLIC_REVENUECAT_API_KEY_TEST"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://luna-backend-1081215078404.us-west1.run.app/api/v1",
        "EXPO_PUBLIC_REVENUECAT_API_KEY": "@EXPO_PUBLIC_REVENUECAT_API_KEY_TEST"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://luna-backend-1081215078404.us-west1.run.app/api/v1",
        "EXPO_PUBLIC_REVENUECAT_API_KEY": "@EXPO_PUBLIC_REVENUECAT_API_KEY_PROD"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "jiahongbinandroid@gmail.com",
        "ascAppId": "6758829951"
      }
    },
    "preview": {
      "ios": {
        "appleId": "jiahongbinandroid@gmail.com",
        "ascAppId": "6758829951"
      }
    }
  }
}
